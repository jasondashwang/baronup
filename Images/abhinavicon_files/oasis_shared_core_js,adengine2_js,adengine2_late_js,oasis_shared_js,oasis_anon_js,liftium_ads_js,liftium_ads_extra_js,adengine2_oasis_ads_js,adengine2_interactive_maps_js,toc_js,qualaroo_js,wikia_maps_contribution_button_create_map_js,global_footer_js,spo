function onAdLoad(slotname,gptEvent,iframe,adCallback,noAdCallback){var adType=getAdType(slotname,gptEvent,iframe),shouldPollForSuccess=false,expectAsyncHop=false,expectAsyncHopWithSlotName=false,expectAsyncSuccess=false,successTimer;function noop(){return;}
